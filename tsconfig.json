{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/graphicals/stage.js", "components/chat/ChatAdmin.js", "components/graphicals/WrapperStageComp.js", "components/chat/ChatCam.js", "components/chat/ChatCam.js", "components/chat/ChatCam.js"],
  "exclude": ["node_modules", "functions"]
}
